{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "https://example.org/schema-1.json",
    "type": "object",
    "properties": {
        "store_branches": {
            "type": "array",
            "items": {
                "type": "object",
                "title": "Store branch",
                "properties": {
                    "id": {
                        "type": "string",
                        "format": "uri"
                    },
                    "name": {
                        "type": "string"
                    },
                    "has_warehouse": {
                        "type": "boolean",
                        "enum": [true, false]
                    },
                    "located_at": {
                        "type": "string"
                    }
                },
                "required": ["id", "name", "has_warehouse"]
            }
        }
    },
    "required": ["store_branches"]
}
