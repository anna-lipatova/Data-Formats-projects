{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "https://example.org/schema-1.json",
    "type": "object",
    "properties": {
        "store_branches": {
            "type": "array",
            "items": {
                "type": "object",
                "title": "Store branch",
                "properties": {
                    "id": {
                        "type": "string",
                        "format": "uri"
                    },
                    "name": {
                        "type": "string"
                    },
                    "has_warehouse": {
                        "type": "boolean",
                        "enum": [true, false]
                    },
                    "Address": {
                        "type": "object",
                        "properties": {
                            "city": {
                                "type": "string"
                            },
                            "street": {
                                "type": "string"
                            },
                            "zip_code": {
                                "type": "string"
                            }
                        },
                        "required": ["city", "street", "zip_code"]
                    },
                    "employees": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "title": "Employee",
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "format": "uri"
                                },
                                "given_name": {
                                    "type": "string"
                                },
                                "family_name": {
                                    "type": "string"
                                }
                            },
                            "required": ["id", "given_name", "family_name"]
                        }
                    },
                    "cash_registers": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "title": "Cash Register",
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "format": "uri"
                                },
                                "identifier": {
                                    "type": "integer"
                                },
                                "is_self_checkout": {
                                    "type": "boolean",
                                    "enum": [true, false]
                                }
                            },
                            "required": ["id", "identifier", "is_self_checkout"]
                        }
                    },
                    "customers": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "title": "Customer",
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "format": "uri"
                                },
                                "given_name": {
                                    "type": "string"
                                },
                                "family_name": {
                                    "type": "string"
                                },
                                "birth_date": {
                                    "type": "string",
                                    "format": "date"
                                },
                                "phone_number": {
                                    "type": "string"
                                }
                            },
                            "required": ["id", "given_name", "family_name", "birth_date", "phone_number"]
                        }
                    }
                },
                "required": ["id", "name", "has_warehouse"]
            }
        }
    },
    "required": ["store_branches"]
}
