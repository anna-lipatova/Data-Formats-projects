{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "https://example.org/schema-cash-register.json",
    "type": "object",
    "properties": {
        "cash_registers": {
            "type": "array",
            "items": {
                "type": "object",
                "title": "Cash Register",
                "properties": {
                    "id": {
                        "type": "string",
                        "format": "uri"
                    },
                    "number": {
                        "type": "integer"
                    },
                    "is_self_checkout": {
                        "type": "boolean"
                    },
                    "leads": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "title": "Employee",
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "format": "uri"
                                },
                                "given_name": {
                                    "type": "string"
                                },
                                "family_name": {
                                    "type": "string"
                                },
                                "works_at": {
                                    "type": "string",
                                    "format": "uri"
                                }
                            },
                            "required": ["id", "given_name", "family_name", "works_at"]
                        }
                    },
                    "located_at": {
                        "type": "object",
                        "title": "Store Branch",
                        "properties": {
                            "id": {
                                "type": "string",
                                "format": "uri"
                            },
                            "name": {
                                "type": "string"
                            },
                            "has_warehouse": {
                                "type": "boolean"
                            },
                            "address": {
                                "type": "object",
                                "properties": {
                                    "city": {
                                        "type": "string"
                                    },
                                    "street": {
                                        "type": "string"
                                    },
                                    "zip_code": {
                                        "type": "string"
                                    }
                                },
                                "required": ["city", "street", "zip_code"]
                            }
                        },
                        "required": ["id", "has_warehouse", "address"]
                    }
                },
                "required": ["id", "number", "is_self_checkout", "located_at"]
            }
        }
    },
    "required": ["cash_registers"]
}
